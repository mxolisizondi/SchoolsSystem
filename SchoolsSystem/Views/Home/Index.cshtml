@{
    ViewBag.Title = "Home Page";
}

<input type="button" value="button" id="btn" />
@section scripts{
    <script>
        $("#btn").click(function () {
            $.ajax({
                url: '/Home/About',
                success: function (data) {
                    $("#renderbody").html(data);
                    console.log('Clicked');
                }
            })
        })
    </script>
}